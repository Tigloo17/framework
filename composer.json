{
    "name": "tigloo17/framework",
    "description": "A framework for personal project development",
    "keywords": ["framework"],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    
    "authors": [
        {
            "name": "Jean-Michel",
            "email": "collajm@hotmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "Tigloo\\": "src/Tigloo/"
        }
    },

    "require": {
        "php": ">=7.1.3",
        "psr/container": "^2.0",
        "guzzlehttp/psr7": "^2.1",
        "m1/env": "^2.2",
        "twig/twig": "^3.3",
        "psr/http-server-middleware": "1.0.x-dev"
    },

    "suggest": {
        "ext-gd": "",
        "ext-json": "",
        "ext-pdo": ""
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
